  author = {Michal Zochowski},
  title = {Intermittent dynamical control},
  journal = {Physica D: Nonlinear Phenomena},
  year = {2000},
  volume = {145},
  pages = {181 - 190},
  number = {3-4},
  abstract = {This paper describes the method of synchronizing slave to the master
	trajectory using intermittent coupling. The information about master
	trajectory is limited only to the coordinates and timings of intersection
	points of that trajectory with the arbitrarily chosen Poincar?section,
	making the coupling sporadic. The application of the method is first
	exemplified on the two identically coupled Rssler and Lorenz oscillators,
	and then is extended to the case when the two Rssler oscillators
	exhibit initially different dynamical properties (different values
	of the control parameters).},
