@ARTICLE{Liu20093122,
  author = {Xiwei Liu and Tianping Chen and Wenlian Lu},
  title = {Consensus problem in directed networks of multi-agents via nonlinear
	protocols},
  journal = {Physics Letters A},
  year = {2009},
  volume = {373},
  pages = {3122 - 3127},
  number = {35},
  abstract = {In this Letter, the consensus problem via distributed nonlinear protocols
	for directed networks is investigated. Its dynamical behaviors are
	described by ordinary differential equations (ODEs). Based on graph
	theory, matrix theory and the Lyapunov direct method, some sufficient
	conditions of nonlinear protocols guaranteeing asymptotical or exponential
	consensus are presented and rigorously proved. The main contribution
	of this work is that for nonlinearly coupled networks, we generalize
	the results for undirected networks to directed networks. Consensus
	under pinning control technique is also developed here. Simulations
	are also given to show the validity of the theories.},
